{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "quotes":[
            "error",
            "single",
            {"avoidEscape":true, "allowTemplateLiterals": false}
        ],
        "semi":"error"
    }
}
